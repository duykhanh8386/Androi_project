<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude caches/temp if needed -->
        <exclude domain="cache" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow everything by default -->
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>
